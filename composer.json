{
    "name": "prezly/slate-php",
    "description": "PHP implementation of the Slate Rich Text Editor data model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Stanciu",
            "email": "victor@prezly.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Prezly\\Slate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prezly\\Slate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev",
            "dev-4.x": "4.x-dev",
            "dev-3.x": "3.x-dev",
            "dev-2.x": "2.x-dev"
        }
    }
}
